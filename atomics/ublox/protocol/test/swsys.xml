<?xml version="1.0" encoding="utf-8" ?>

<swsys name="target">
<!--    <expected_interface name="TLM1" dev_path="/dev/ttyS0"/>-->

    <bus name="protocol_bus" eq_channel="1" max_topics="32">
        <dir name="gnss1"/>
    </bus>

    <task type="flow" config="ubx_parsing.xml" name="ubx_parsing" priority="4" clk_method="freerun">
        <connect>
<!--            <in alias="omega, a">nav_bus/$alias</in>-->
            <out alias="omega_bias, q0, imu_reset">nav_bus/$alias</out>
        </connect>
    </task>

    <task type="ibr" config="ibr_ubx.xml" name="gnss1" priority="5" clk_method="freerun">
        <connect>
            <out alias="rx_frame">itb:/protocol_bus/gnss1/</out>
        </connect>
    </task>

</swsys>
